#ifndef RCC_DRIVER_RCC_CONFIG_H_
#define RCC_DRIVER_RCC_CONFIG_H_

#define RCC_HSE 0
#define RCC_HSI 1
#define RCC_PLL 2
#define RCC_LSE 3
#define RCC_LSI 4
/* Choose clock from here */
#define RCC_Clock RCC_HSE

#define HSI_Select 0b00
#define HSE_Select 0b01
#define PLL_Select 0b10

#endif /* RCC_DRIVER_RCC_CONFIG_H_ */
